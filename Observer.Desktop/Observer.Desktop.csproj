<Project Sdk="Microsoft.NET.Sdk.Razor">
  
  <PropertyGroup>
	<OutputType>WinExe</OutputType>
	  <TargetFramework>net7</TargetFramework>
	  <ApplicationIcon>favicon.ico</ApplicationIcon>
  </PropertyGroup>
  
  <ItemGroup>
    <Content Remove="Pages\BasePage.razor" />
  </ItemGroup>



	<ItemGroup>
		<PackageReference Include="Autofac.Extensions.DependencyInjection" Version="7.2.0" />
		<PackageReference Include="LeaderAnalytics.Core" Version="3.3.3" />
		<PackageReference Include="LeaderAnalytics.LeaderPivot.Blazor" Version="2.2.7" />
		<PackageReference Include="LeaderAnalytics.Vyntix.Fred.Downloader" Version="1.1.1" />
		<PackageReference Include="Microsoft.Extensions.Hosting" Version="7.0.1" />
		<PackageReference Include="Microsoft.Extensions.Logging" Version="7.0.0" />
		<PackageReference Include="Microsoft.Extensions.Logging.Console" Version="7.0.0" />
		<PackageReference Include="MudBlazor" Version="6.11.0" />
		<PackageReference Include="Photino.Blazor" Version="2.6.0" />
		<PackageReference Include="Serilog.AspNetCore" Version="7.0.0" />
		<PackageReference Include="Serilog.Sinks.File" Version="5.0.0" />
	</ItemGroup>
	<ItemGroup>
		<Content Update="wwwroot\**">
			<CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
		</Content>
	</ItemGroup>

	<ItemGroup>
		<None Update="favicon.ico">
			<CopyToOutputDirectory>Always</CopyToOutputDirectory>
		</None>
	</ItemGroup>
</Project>
